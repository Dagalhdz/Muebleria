<a class="btn btn-outline-primary mb-4" href="/empleados/cajeros/add"><i class="fas fa-plus-circle"></i> Agregar Nuevo cajero</a>
<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th class="text-uppercase text-center" scope="col">id</th>
      <th class="text-uppercase text-center" scope="col">nombre</th>
      <th class="text-uppercase text-center" scope="col">telefono</th>
      <th class="text-uppercase text-center" scope="col">email</th>
      <th class="text-uppercase text-center" scope="col">Editar</th>
      <th class="text-uppercase text-center" scope="col">eliminar</th>
    </tr>
  </thead>
  <tbody>
     {{#each cajero}}
    <tr>
      <th class="align-middle text-center" scope="row">{{id}}</th>
      <td class="align-middle text-center">{{nombre}}</td>
      <td class="align-middle text-center">{{telefono}}</td>
      <td class="align-middle text-center">{{email}}</td>
      <td class="align-middle text-center"><a class="text-success" style="font-size: 2rem;" href="/empleados/edit/{{id}}"><i class="fas fa-edit"></i></a></td>
      <td class="align-middle text-center"><a class="text-danger" style="font-size: 2rem;" href="/empleados/delete/{{id}}"><i class="fas fa-trash"></i></a></td>
    </tr>
    {{/each}}
  </tbody>
</table>